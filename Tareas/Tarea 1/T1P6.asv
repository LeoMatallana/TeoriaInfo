% Author: Daniel Cubides and Leonardo Matallana
% Description: Script to remove val
% Date: 22/08/2015
filename = 'cut.mp4';
[soundData,samplingFrec] = audioread(filename);
[r,c] = size(soundData);

for i = 1:r
    X = randi(100);
    if(X<=99)
        soundData(i,1)=0;
        soundData(i,2)=0;
    end    
end
sound(soundData,samplingFrec);
plot(soundData);
audiowrite('cutP6x99.mp4',cut,samplingFrec);
